# Package manager: npm | pnpm | yarn
NPM_CLIENT=npm

# Nuxt major version preference (3 by default). To try Nuxt 4, set 4.
NUXT_MAJOR=3

# Pin Nuxt version (defaults to 3.17.4). Adjust as needed.
NUXT_VERSION=3.17.4

# Additional args passed to `nuxi init` (e.g., edge/next templates). Optional.
# Example (subject to nuxi support):
# NUXI_INIT_ARGS="--edge"
NUXI_INIT_ARGS=

# Auto initialize project if missing
AUTO_INIT=true

# Dev server host/port
HOST=0.0.0.0
NUXT_PORT=3000

# Library versions (override as needed). Use semver ranges or "latest".
PINIA_VERSION=^2
PINIA_NUXT_VERSION=^0.5
NUXT_I18N_VERSION=^8
HEADLESSUI_VUE_VERSION=^1.7.19
HEROICONS_VUE_VERSION=^2
FLYONUI_VERSION=latest
NUXT_TAILWINDCSS_VERSION=^6
ICONIFY_TAILWIND4_VERSION=^1


